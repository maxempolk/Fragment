[
  {
    "label": "Run FastAPI with Uvicorn",
    "command": "fastapi dev",
    "env": {
      "PYTHONPATH": ".venv/bin/python"
    },
    "use_new_terminal": false,
    "allow_concurrent_runs": false,
    "reveal": "always",
    "hide": "never",
    "shell": "system",
    "show_summary": true,
    "show_output": true
  },
  {
    "label": "Build image",
    "command": "docker build -t fastapi-backend .",
    "use_new_terminal": false,
    "allow_concurrent_runs": false,
    "reveal": "always",
    "hide": "never",
    "shell": "system",
    "show_summary": true,
    "show_output": true
  }
]
